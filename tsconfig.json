{
  "compilerOptions": {
    "module": "CommonJS",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "Node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "./",
    "allowSyntheticDefaultImports":true
  },
  "lib": ["es2015"],
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}